{
    "buildCommand": "cd frontend && npm run dev",
    "outputDirectory": "frontend/.next",
    "framework": "nextjs",
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "/backend/api/:path*"
      },
      {
        "source": "/ml/:path*",
        "destination": "/ml/api/:path*"
      }
    ]
  }